	F.Data.Dictionary.Close("dictPO")
	
	F.Data.Dictionary.CreateFromSQL("dictPOLine","conts","select PURCHASE_ORDER+'-'+Left(RECORD_NO,3),DATE_DUE_LINE from V_PO_LINES")
	F.Data.Dictionary.SetDefaultReturn("dictPOLine","1900-01-01")
	
	F.Data.DataTable.FillFromDictionary("dtOps","dictPOLine","PO_NUMBER","PO_DUE_DATE")
	
	F.Data.Dictionary.Close("dictPOLine")
	
	F.Data.DataTable.AddExpressionColumn("dtOps","PO","String","Substring([PO_NUMBER],1,7)")
	
	F.Data.Dictionary.CreateFromSQL("dictBuyer","conts","select PURCHASE_ORDER,BUYER from V_PO_HEADER")
	F.Data.Dictionary.SetDefaultReturn("dictBuyer","")
	
	F.Data.DataTable.FillFromDictionary("dtOps","dictBuyer","PO","BUYER")
	
	F.Data.Dictionary.Close("dictBuyer")
	
	F.ODBC.Connection!conts.Close
	
	'F.Data.DataTable.RemoveColumn("dtOps","JSS")
	F.Data.DataTable.RemoveColumn("dtOps","PO")
	
	F.Data.DataTable.AddExpressionColumn("dtOps","Ready","String","IIF(HOURS_ACTUAL >= HOURS_ESTIMATED,'Y',IIF(QTY_ONHAND >= HOURS_ESTIMATED,'Y','N'))")
	
	F.Data.DataView.Create("dtOps","dvOps",22)
	
	F.Intrinsic.Control.If(V.DataTable.dtOpsTemp.Exists)
		F.Data.DataTable.Close("dtOpsTemp")
	F.Intrinsic.Control.EndIf
	
	F.Data.DataView.ToDataTableDistinct("dtOps","dvOps","dtOpsTemp","JS",True)
	F.Data.DataView.Close("dtOps","dvOps")
	
	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtOpsTemp.RowCount--,1)
		F.Intrinsic.String.Build("JS = '{0}'",V.DataTable.dtOpsTemp(V.Local.iCnt).JS!FieldValTrim,V.Local.sFilter)
