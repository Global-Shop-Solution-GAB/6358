	F.Intrinsic.Control.CallSub(OpsGv)

F.Intrinsic.Control.Else
	V.Local.sSQL.Set("Select A.SEQ,A.LMO,A.PART,A.LOCN,A.DESCRIPTION,A.FLAG_CLOSED,A.OPERATION,A.DATE_START,A.DATE_DUE,A.HOURS_ESTIMATED,A.HOURS_ACTUAL,A.DOLLARS_ESTIMATED,A.DOLLARS_ACTUAL,A.UNITS_OPEN,A.UNITS_COMPLETE,B.QTY_ONHAND,B.QTY_REQUIRED,B.QTY_ONORDER_PO,'' as PO_NUMBER,Cast('1900-01-01' as Date) as PO_DUE_DATE,'' as BUYER,A.JOB+A.SUFFIX+A.SEQ as JSS,A.JOB+A.SUFFIX as JS from V_JOB_OPERATIONS A left join V_INVENTORY_MSTR B on A.PART = B.PART and A.LOCN = B.LOCATION left join V_JOB_HEADER C on A.JOB = C.JOB and A.SUFFIX = C.SUFFIX where A.LMO = 'M' and Left(SEQ,2) <> '99' and C.DATE_CLOSED = '1900-01-01';")
	
	F.ODBC.Connection!conts.OpenCompanyConnection
	
	F.Data.DataTable.CreateFromSQL("dtOps","conts",V.Local.sSQL,True)
	
	F.Data.Dictionary.CreateFromSQL("dictPO","conts","Select JOB+SUFFIX+SEQ as JSS,RTRIM(PART) as PO_NUMBER from V_JOB_DETAIL where SEQUENCE_KEY = '0000' and TRAN = '52'")
	F.Data.Dictionary.SetDefaultReturn("dictPO","")
	
