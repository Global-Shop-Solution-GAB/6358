	gui.FrmAttributes.GsGcAttributes.SetColumnProperty("gvAttr", "ATTRNO", v.Enum.ColumnPropertyNames!HeaderFontBold, True)
	gui.FrmAttributes.GsGcAttributes.SetColumnProperty("gvAttr", "ATTRTITLE", v.Enum.ColumnPropertyNames!HeaderFontBold, True)
	gui.FrmAttributes.GsGcAttributes.SetColumnProperty("gvAttr", "Check", v.Enum.ColumnPropertyNames!HeaderFontBold, True)
	
