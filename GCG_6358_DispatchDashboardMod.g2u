'6358 change to get location from V_JOB_HEADER instead of V_JOB_OPERATIONS
	F.Intrinsic.String.Build("Select A.SEQ,A.LMO,A.PART, H.LOCATION LOCN, A.DESCRIPTION,A.FLAG_CLOSED,A.OPERATION,A.DATE_START,A.DATE_DUE, A.HOURS_ESTIMATED,A.HOURS_ACTUAL,A.DOLLARS_ESTIMATED,A.DOLLARS_ACTUAL,A.UNITS_OPEN,A.UNITS_COMPLETE,B.QTY_ONHAND,B.QTY_REQUIRED,B.QTY_ONORDER_PO,'' as PO_NUMBER,Cast('1900-01-01' as Date) as PO_DUE_DATE,'' as BUYER,A.JOB+A.SUFFIX+A.SEQ as JSS from V_JOB_OPERATIONS A left join V_JOB_HEADER H on A.JOB = H.JOB and A.SUFFIX = H.SUFFIX left join V_INVENTORY_MSTR B on A.PART = B.PART and H.LOCATION=B.LOCATION where A.JOB = '{0}' and A.SUFFIX = '{1}'",V.Args.sWO,V.Args.sSuff,V.Local.sSQL)
	
	F.ODBC.Connection!conts.OpenCompanyConnection
	
	F.Data.DataTable.CreateFromSQL("dtOps","conts",V.Local.sSQL,True)
	
	F.Data.Dictionary.CreateFromSQL("dictPO","conts","Select JOB+SUFFIX+SEQ as JSS,RTRIM(PART) as PO_NUMBER from V_JOB_DETAIL where SEQUENCE_KEY = '0000' and TRAN = '52'")
	F.Data.Dictionary.SetDefaultReturn("dictPO","")
	
	F.Data.DataTable.FillFromDictionary("dtOps","dictPO","JSS","PO_NUMBER")
	
	F.Data.Dictionary.Close("dictPO")
	
	F.Data.Dictionary.CreateFromSQL("dictPOLine","conts","select PURCHASE_ORDER+'-'+Left(RECORD_NO,3),DATE_DUE_LINE from V_PO_LINES")
	F.Data.Dictionary.SetDefaultReturn("dictPOLine","1900-01-01")
	
	F.Data.DataTable.FillFromDictionary("dtOps","dictPOLine","PO_NUMBER","PO_DUE_DATE")
	
	F.Data.Dictionary.Close("dictPOLine")
	
	F.Data.DataTable.AddExpressionColumn("dtOps","PO","String","Substring([PO_NUMBER],1,7)")
	
	F.Data.Dictionary.CreateFromSQL("dictBuyer","conts","select PURCHASE_ORDER,BUYER from V_PO_HEADER")
	F.Data.Dictionary.SetDefaultReturn("dictBuyer","")
	
	F.Data.DataTable.FillFromDictionary("dtOps","dictBuyer","PO","BUYER")
	
