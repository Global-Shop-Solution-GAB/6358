		F.Intrinsic.Control.ElseIf(V.Local.iMatReadyCnt,>,0,"AND",V.Local.iMatReadyCnt,<,V.Local.iMatCnt)
			F.Intrinsic.String.Build("JOB+SFX = '{0}'",V.DataTable.dtOpsTemp(V.Local.iCnt).JS!FieldValTrim,V.Local.sFilter)
			F.Data.DataView.Create("dtWC","dvWCMatReady",22,V.Local.sFilter,"")
			F.Intrinsic.Control.If(V.DataView.dtWC!dvWCMatReady.RowCount,<>,0)
				F.Data.DataView.SetValue("dtWC","dvWCMatReady",-1,"READY","P")
			F.Intrinsic.Control.EndIf
